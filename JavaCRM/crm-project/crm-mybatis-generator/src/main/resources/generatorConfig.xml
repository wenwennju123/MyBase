<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <!--指定mysql数据库驱动，提供jar包所在绝对路径-->
    <!--导入属性配置，默认从根目录下寻找-->
    <properties resource="generator.properties"/>

    <!--指定特定数据库的jdbc驱动jar包的位置-->
    <classPathEntry location="${jdbc.driverLocation}"/>

    <context id="default" targetRuntime="MyBatis3">

        <!-- optional，旨在创建class时，对注释进行控制，false生成注释,true无注释 -->
        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>

        <!--jdbc的数据库连接 -->
        <jdbcConnection
                driverClass="${jdbc.driverClass}"
                connectionURL="${jdbc.connectionURL}"
                userId="${jdbc.userId}"
                password="${jdbc.password}">
        </jdbcConnection>

        <!-- 非必需，类型处理器，在数据库类型和java类型之间的转换控制-->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- entity
            实体类,模型生成器,
            用来生成含有主键key的类，记录类 以及查询Example类
            targetProject     指定生成到的项目工程的所在路径，指定java源文件java文件夹
            targetPackage     指定生成的model，即实体类 entity 所在的包名
        -->
        <javaModelGenerator targetPackage="com.leowork.crm.workbench.entity"
                            targetProject="E:/JavaCRM/crm-project/crm/src/main/java">
            <!-- 是否允许子包，即targetPackage.schemaName.tableName -->
            <property name="enableSubPackages" value="false"/>
            <!-- 是否对model添加 构造函数 true添加，false不添加-->
            <property name="constructorBased" value="false"/>
            <!-- 是否对类CHAR类型的列的数据进行trim操作 -->
            <property name="trimStrings" value="true"/>
            <!-- 建立的Model对象是否 不可改变  即生成的Model对象不会有 setter方法，只有构造方法 -->
            <property name="immutable" value="false"/>
        </javaModelGenerator>

        <!--Mapper
            映射文件生成所在的目录 为每一个数据库的表生成对应的SqlMap文件 -->
        <sqlMapGenerator targetPackage="com.leowork.crm.workbench.mapper"
                         targetProject="E:/JavaCRM/crm-project/crm/src/main/java">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <!-- XML
            客户端代码，生成易于使用的针对Model对象和XML配置文件 的代码
            生成mapper接口，提供常用方法
                type="ANNOTATED-MAPPER",生成Java Model 和基于注解的Mapper对象
                type="MIXED MAPPER",生成基于注解的Java Model 和相应的Mapper对象
                type="XML-MAPPER",生成SQLMap XML文件和独立的Mapper接口
        -->
        <javaClientGenerator targetPackage="com.leowork.crm.workbench.mapper"
                             targetProject="E:/JavaCRM/crm-project/crm/src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--
            精确提供目标的表，重复生成会覆盖 mapper 以及 mapper.xml
            tableName 表的名字
            domainObjectName 生成实体类的类名
        -->

